{
  "name": "hunt-map",
  "version": "2.20.0",
  "homepage": "https://hunt-map.info",
  "private": true,
  "author": "Findoss",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "start": "cross-env NODE_ENV=production webpack --mode production",
    "cache": "node ./scripts/updateCache.js LD & node ./scripts/updateCache.js SB",
    "static": "node ./scripts/staticServer.js",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "deploy": "sh ./scripts/deploy.sh",
    "update": "sh ./scripts/update.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "firebase": "6.0.2",
    "firebaseui": "4.0.0",
    "leaflet": "1.5.1",
    "leaflet-draw": "^1.0.4",
    "leaflet-easyprint": "^2.1.9",
    "leaflet-responsive-popup": "^0.6.4",
    "leaflet.fullscreen": "^1.6.0",
    "static-server": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "firebase-admin": "^8.10.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
